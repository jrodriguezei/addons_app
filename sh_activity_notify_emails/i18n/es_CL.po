#. module: sh_activity_notify_emails
#: model:mail.template,body_html:sh_activity_notify_emails.send_activity_notification_assignee
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 13px;\">\n"
"                    Dear <b><t t-out=\"object.user_id.partner_id.name or ''\"></t></b>,\n"
"                    <br>\n"
"                    <br>\n"
"                    <t t-if=\"ctx.get('done_activity')\">\n"
"                        Activity # <t t-out=\"object.activity_type_id.name or ''\"></t> of document <t t-out=\"object.res_name or ''\"></t> has been done by <t t-out=\"ctx['user'] or ''\"></t> .\n"
"                        <br>\n"
"                        <br>\n"
"                        <strong> Feedback: </strong><t t-out=\"object.get_feedback() or ''\"></t>\n"
"                    </t>\n"
"                    <t t-if=\"ctx.get('edit_activity')\">\n"
"                        Activity # <t t-out=\"object.activity_type_id.name or ''\"></t> of document <t t-out=\"object.res_name or ''\"></t> has been modified by <t t-out=\"ctx['user'] or ''\"></t>.\n"
"                        <br>\n"
"                        <br>\n"
"                        <strong> Summary: </strong><t t-out=\"object.summary or ''\"></t>\n"
"                        <br>\n"
"                        <strong> Description: </strong><t t-out=\"object.get_note() or ''\"></t>\n"
"                        <br>\n"
"                        <strong> Due Date: </strong><t t-out=\"object.date_deadline or ''\"></t>\n"
"                    </t>\n"
"                    <t t-if=\"ctx.get('unlink_activity')\">\n"
"                        Activity # <t t-out=\"object.activity_type_id.name or ''\"></t> of document <t t-out=\"object.res_name or ''\"></t> has been cancelled by <t t-out=\"ctx['user'] or ''\"></t>.\n"
"                        <br>\n"
"                        <br>\n"
"                    </t>\n"
"                </p>\n"
"                <div style=\"display: inline-block; margin: 15px 0px; text-align: center;\">\n"
"                    <a t-att-href=\"'/mail/view?model=%s&amp;res_id=%s' % (object.res_model, object.res_id)\" target=\"_blank\" style=\"padding: 5px 10px; color: #ffffff; text-decoration: none; background-color: #875a7b; border: 1px solid #875a7b; border-radius: 3px;\">\n"
"                        View\n"
"                    </a>\n"
"                </div>\n"
"            </div>\n"
"        "
msgstr ""

